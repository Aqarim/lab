#include <stdio.h>
#include <stdlib.h>

int main() {
	int a=0, b=10;
	float array[20];
	for(int i=0; i<20; ++i) {
		array[i]=(double)rand()*(b - a)/RAND_MAX + a;
		printf("%lf ",array[i]);
	}
	printf("\n");
	for(int i=1; i<19; ++i) {
		if(array[i]>array[i+1] & array[i]>array[i-1]) printf("%lf ",array[i]);
	}
}
