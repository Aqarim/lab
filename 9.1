#include <stdio.h>
#include <stdlib.h>

const int a=10, b=20;

int random (int N) {
	return rand()%N;
}

int main() {
	int m[a][b];
	int x=10, y=0;
	
	//заполнение матрицы рандомными числами//
	for(int i=0; i<a; i++) {
		for(int j=0; j<b; j++) {
			m[i][j]=random(x-y+1) + y;
		}
	}
	
	//вывод матрицы на экран//
	printf("Matrix before:\n");
	for(int i=0; i<a; i++) { 
 		for (int j = 0; j<b; j++) {
       		printf("%4d",m[i][j]);
       }
	printf("\n");
	}
	
	//замена сток местами//
	for(int i=0; i<a; i+=2) {
		for(int j=0; j<b; j++) {
			int tmp=m[i][j];
			m[i][j]=m[i+1][j];
			m[i+1][j]=tmp;
		}
	}
	printf("\n");
	
	//вывод матрицы после изменений//
	printf("Matrix after:\n");
	for(int i=0; i<a; i++) { 
 		for (int j = 0; j<b; j++) {
       		printf("%4d",m[i][j]);
       }
	printf("\n");
	}
}
